{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs/:match*",
      "destination": "https://docs.astro.build/:match*",
      "permanent": false
    },
    {
      "source": "/posts/:match*",
      "destination": "blog/:match*",
      "permanent": false
    },
    {
      "source": "/chat",
      "destination": "https://discord.gg/grF4GTXXYm"
    }
  ],
  "rewrites": [
    {
      "source": "/play/:match*",
      "destination": "https://play.astro.build/play/:match*"
    }
  ]
}
