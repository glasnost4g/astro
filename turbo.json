{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/dist/**",
        "!**/vendor/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "test:templates": {
      "outputs": []
    },
    "benchmark": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
